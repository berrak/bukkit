#!/bin/sh
#
##############################################################
# Do not edit. Managed by Puppet. Changes will be wiped out. #
##############################################################
# Script to make and make install hosts all
# source files for project <%= projectname %>
#
echo "---------------------------------------------------"
echo -n "*** START MAKE & MAKE INSTALL: "
`date +%Y%m%d.%T`
echo "---------------------------------------------------"
cd /home/<%= username %>/<%= projectname %>
export COBCPY=/home/<%= username %>/<%= projectname %>/<%= copybookname %>
export COB_LIBRARY_PATH=/home/<%= username %>/<%= projectname %>/<%= libraryname %>
echo "-----------------Build Environment-----------------"
env | grep COB
echo "---------------------------------------------------"
make
make install
echo "-----------------Build Environment-----------------"
env | grep COB
echo "---------------------------------------------------"
echo -n "*** BUILD AND INSTALL FINISHED: "
`date +%Y%m%d.%T`
echo "---------------------------------------------------"